{
  "name": "node-i2c",
  "version": "0.2.3",
  "description": "Native bindings for i2c-dev. Plays well with Raspberry Pi and BeagleBone.",
  "main": "main.js",
  "author": "Kelly Korevec",
  "homepage": "https://github.com/fchen7/node-i2c",
  "repository": {
    "type": "git",
    "url": "https://github.com/fchen7/node-i2c.git"
  },
  "license": "BSD-3-Clause-Attribution",
  "dependencies": {
    "bindings": "1.2.1",
    "coffee-script": "1.9.1",
    "nan": "~2.3.5",
    "node-pre-gyp": "latest",
    "repl": "0.1.3",
    "underscore": "1.8.2"
  },
  "engine": "node >= 0.12.0",
  "binary": {
    "module_name": "node-i2c",
    "module_path": "build/{configuration}/",
    "host": "https://node-i2c.s3-ap-northeast-1.amazonaws.com/"
  },
  "scripts": {
    "install": "node-pre-gyp install --fallback-to-build"
  },
  "devDependencies": {
    "aws-sdk": "latest"
  }
}
